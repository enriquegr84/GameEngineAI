<DebugOptions>
	<!-- ResCache -->
	<ResCache useDevelopmentDirectories="no" />
	<!-- Render 
		lighting = test ambient/diffuse/specularity in RGB & all anisotropic
		profiler = True if graphical profiler should be displayed
		hwSkinning = True if hardware skinning should be enabled
		
	-->
	<RenderDebug lightining="no" profiler="no" hwSkinning="no" materialDebugging="no" />
	<!-- Physics -->
	<PhysicsDebug DrawWireFrame="no" DrawContactPoints="no" />
	<!-- Race specifics 
		materialDebug = Terrain specific slowdown is enabled
		trackDebug = Track debugging is enabled
		checkDebug = "Check Structures should be debugged"
		cameraDebug = 0: normal cameral; 1: being high over the kart; 2: on ground level.
		slipstreamDebug = slipstream debugging is activated
		fltDebug = follow the leader debug information should be printed
		tutorialDebug = currently developed tutorial debugging is enabled
	-->
	<Race 	materialDebug="no" trackDebug="no" checkDebug="no" cameraDebug="0" slipstreamDebug="no" fltDebug="no" 
			tutorialDebug="no" startScreen="yes" raceNow="no" />
	
</DebugOptions>